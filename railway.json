{
  "build": {
    "builder": "NIXPACKS"
  },
  "deploy": {
    "startCommand": "java -jar -Dspring.profiles.active=production -Dserver.port=$PORT target/demo-0.0.1-SNAPSHOT.jar",
    "healthcheckPath": "/health",
    "healthcheckTimeout": 900,
    "healthcheckInterval": 10,
    "restartPolicyType": "ON_FAILURE",
    "restartPolicyMaxRetries": 30
  }
}